<manifest>
	<id>SprocketComputers.PhotoViewer</id>
	<name>Photo Viewer</name>
	<publisher>Sprocket Computers</publisher>
	<version>0.1</version>
	<icon>%app_path%photoviewer.svg</icon>
	<index>index.php</index>
	<date>1493614800</date>
	<description><![CDATA[View photos and images stored on your computer.]]></description>
	<types>
		<jpg>
			<icon><![CDATA[%FLY.RESOURCE.URL.ICONS%type/image.svg]]></icon>
			<description><![CDATA[JPEG Image]]></description>
			<action><![CDATA[file=%FLY.CURRENT_PATH%]]></action>
			<mime>image/jpeg</mime>
		</jpg>
		<png>
			<icon><![CDATA[%FLY.RESOURCE.URL.ICONS%type/image.svg]]></icon>
			<description><![CDATA[PNG Image]]></description>
			<action><![CDATA[file=%FLY.CURRENT_PATH%]]></action>
			<mime>image/png</mime>
		</png>
		<gif>
			<icon><![CDATA[%FLY.RESOURCE.URL.ICONS%type/image.svg]]></icon>
			<description><![CDATA[GIF Image]]></description>
			<action><![CDATA[file=%FLY.CURRENT_PATH%]]></action>
			<mime>image/gif</mime>
		</gif>
		<bmp>
			<icon><![CDATA[%FLY.RESOURCE.URL.ICONS%type/image.svg]]></icon>
			<description><![CDATA[Bitmap Image]]></description>
			<action><![CDATA[file=%FLY.CURRENT_PATH%]]></action>
		</bmp>
		<svg>
			<icon><![CDATA[%FLY.RESOURCE.URL.ICONS%type/image.svg]]></icon>
			<description><![CDATA[SVG Image]]></description>
			<action><![CDATA[file=%FLY.CURRENT_PATH%]]></action>
			<mime>image/svg+xml</mime>
		</svg>
	</types>
	<mime>image/*</mime>
	<window>
		<title>Photo Viewer</title>
		<width>640</width>
		<height>480</height>
		<x>auto</x>
		<y>auto</y>
		<expand>true</expand>
		<resize>true</resize>
	</window>
</manifest>